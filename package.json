{
  "name": "khanhub-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "npm --prefix apps/web run dev",
    "dev:surgical": "npm --prefix apps/surgical run dev",
    "dev:enterprise": "npm --prefix apps/enterprise run dev",
    "dev:jobs": "npm --prefix apps/job run dev",
    "dev:transport": "npm --prefix apps/transport run dev",
    "build:web": "npm --prefix apps/web run build",
    "build:surgical": "npm --prefix apps/surgical run build",
    "build:enterprise": "npm --prefix apps/enterprise run build",
    "build:jobs": "npm --prefix apps/job run build",
    "build:transport": "npm --prefix apps/transport run build"
  },
  "dependencies": {
    "@vercel/analytics": "^1.6.1",
    "lottie-web": "^5.13.0",
    "next-sitemap": "^4.2.3",
    "react-icons": "^5.5.0"
  }
}
